{
    "name": "phaser",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/phaser/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/phaser",
                "main": "libs/phaser/src/index.ts",
                "tsConfig": "libs/phaser/tsconfig.lib.json",
                "assets": ["libs/phaser/*.md"]
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/phaser/**/*.ts"]
            }
        }
    },
    "tags": []
}
